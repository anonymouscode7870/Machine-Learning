info = {
    "name":"abc",
    "age": 6,
    "subject":["maths", "science"],
    88: "hello ji"
}
print(info)
print(info["name"])
info["age"] = 8
print(info["age"])





# get all keys
key = info.keys()
print(key)
print(type(key))

key1 = list(info.keys())
print(key1)
print(type(key1))


# get all values
val = info.values()
print(val)
print(type(val))

val1 = list(info.values())
print(val1)
print(type(val1))


# get key values pairs
print(info.items())


# get method
print(info.get("name")) # another way to access values if key does not exist then it prints none and program do not terminates


# update method add new item
info["fds"] = "dw"
# or
info.update(
    {
        "new":"old"
    }
)


print(info)





